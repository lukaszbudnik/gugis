<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %c{1.} - %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="io.github.lukaszbudnik.guice.multibindinginterceptor.ioc.ReplicatorInterceptor" level="debug"
                additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Logger name="io.github.lukaszbudnik.guice.multibindinginterceptor.ioc.test" level="trace" additivity="false">
            <AppenderRef ref="Console"/>
        </Logger>
        <Root level="info">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>